      <main class="new-schedule-main-content">
        <h1 class="text3">Schedule</h1>
        <p class="goals-note">
          *Tasks have been sorted in order of priority according to the EDF algorithm, you can change the
          priorities and save to create the schedule.
        </p>
        <div id="goalsList">
          <% goals.forEach(function(item) { %>
          <div class="goal-item">
            <div class="goal-header">
              <div class="goal-header-text">
                <h3><%= item.title %></h3>
                <p class="priority"><%= item.priority.toUpperCase() %> PRIORITY</p>
                <p class="text7">Deadline: <%= item.deadline %></p>
                <p class="text7">Date: <%= item.date %></p>
              </div>

