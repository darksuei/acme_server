      <a href="/"
        ><button class="btn3">
          <svg class="svg1" xmlns="http://www.w3.org/2000/svg" height="12" width="12" viewBox="0 0 12 12">
            <title>grid interface</title>
            <g stroke-width="1" fill="#ffffff" stroke="#ffffff" class="nc-icon-wrapper">
              <rect
                x="0.5"
                y="0.5"
                width="4"
                height="4"
                fill="none"
                stroke="#ffffff"
                stroke-linecap="round"
                stroke-linejoin="round"
              ></rect>
              <rect
                x="7.5"
                y="0.5"
                width="4"
                height="4"
                fill="none"
                stroke-linecap="round"
                stroke-linejoin="round"
                data-color="color-2"
              ></rect>
              <rect
                x="0.5"
                y="7.5"
                width="4"
                height="4"
                fill="none"
                stroke-linecap="round"
                stroke-linejoin="round"
                data-color="color-2"
              ></rect>
              <rect
                x="7.5"
                y="7.5"
                width="4"
                height="4"
                fill="none"
                stroke="#ffffff"
                stroke-linecap="round"
                stroke-linejoin="round"
              ></rect>
            </g>
          </svg>
